{
  "name": "reason-guest-login-oidc-client",
  "version": "0.0.0",
  "description": "My Project",
  "esy": {
    "build": "dune build -p #{self.name}",
    "release": {
      "releasedBinaries": [
        "ReasonGuestLoginOidcClientApp.exe"
      ]
    }
  },
  "buildDirs": {
    "test": {
      "imports": [
        "Library = require('reason-guest-login-oidc-client/library')",
        "Rely = require('rely/lib')"
      ],
      "bin": {
        "RunTests.exe": "RunTests.re"
      },
      "flags": [
        ":standard",
        "-w",
        "-9"
      ],
      "rawBuildConfig": [
        "(ocamlopt_flags -linkall -g)",
        "(package test-reason-guest-login-oidc-client)",
        "(modules :standard RunTests)"
      ]
    },
    "routes": {
      "imports": []
    },
    "library": {
      "imports": [
        "Lwt = require('lwt')",
        "Uri = require('uri')",
        "uuidm = require('yojson')",
        "Str = require('str')",
        "Routes = require('reason-guest-login-oidc-client/routes')",
        "Oidc = require('reason-guest-login-oidc-client/oidc')"
      ]
    },
    "jose": {
      "imports": [
        "Containers = require('containers')",
        "Yojson = require('yojson')",
        "Nocrypto = require('nocrypto')",
        "X509 = require('x509')",
        "Base64 = require('base64')",
        "Zarith = require('zarith')",
        "Hex = require('hex')"
      ]
    },
    "oidc": {
      "imports": [
        "Lwt = require('lwt')",
        "Logs = require('logs')",
        "Uri = require('uri')",
        "Yojson = require('yojson')",
        "Str = require('str')",
        "Containers = require('containers')",
        "Jose = require('reason-guest-login-oidc-client/jose')"
      ]
    },
    "executable": {
      "imports": [
        "Logs = require('logs')",
        "Logs_fmt = require('logs/fmt')",
        "Fmt_tty = require('fmt/tty')",
        "Lwt = require('lwt')",
        "Library = require('reason-guest-login-oidc-client/library')",
        "Oidc = require('reason-guest-login-oidc-client/oidc')"
      ],
      "bin": {
        "ReasonGuestLoginOidcClientApp.exe": "ReasonGuestLoginOidcClientApp.re"
      },
      "rawBuildConfig": [
        "(package reason-guest-login-oidc-client)",
        "(ocamlopt_flags -O3)"
      ],
      "rawBuildConfigFooter": [
        "(env (docker (flags (:standard -ccopt -static -verbose))))"
      ]
    }
  },
  "scripts": {
    "test": "esy b dune runtest"
  },
  "dependencies": {
    "@opam/base64": "*",
    "@opam/containers": "*",
    "@opam/cstruct-lwt": "*",
    "@opam/dune": "*",
    "@opam/fmt": "*",
    "@opam/hex": "*",
    "@opam/logs": "*",
    "@opam/lwt": "*",
    "@opam/nocrypto": "*",
    "@opam/uri": "*",
    "@opam/uuidm": "*",
    "@opam/x509": "0.6.3",
    "@opam/yojson": "*",
    "@opam/zarith": "*",
    "@esy-ocaml/reason": "*",
    "refmterr": "*",
    "ocaml": "~4.7.0",
    "@pesy/esy-pesy": "0.1.0-alpha.3",
    "@reason-native/rely": "*",
    "@reason-native-web/morph": "*",
    "@reason-native-web/morph_server_http": "*"
  },
  "devDependencies": {
    "@opam/merlin": "*",
    "@opam/ocamlformat": "*"
  },
  "resolutions": {
    "@pesy/esy-pesy": "esy/pesy#2416a5e9f874f334120bec46776e0eece06d7a45",
    "@opam/opium_core": "rgrinberg/opium:opium_core.opam#287b83f85bcf6c75d503b1edab77c7ec524f9090",
    "@opam/cstruct": "3.3.0",
    "pesy": "esy/pesy#ba6359f25621280a8105d2ffc99d75d849c0d95a",
    "@opam/nocrypto": "mirleft/ocaml-nocrypto:opam#ed7bb8d",
    "@opam/httpaf-lwt-unix": "anmonteiro/httpaf:httpaf-lwt-unix.opam#76b461bed081c64908fb1fdfa076ab2c936ca622",
    "@opam/httpaf-lwt": "anmonteiro/httpaf:httpaf-lwt.opam#76b461bed081c64908fb1fdfa076ab2c936ca622",
    "@opam/httpaf": "anmonteiro/httpaf:httpaf.opam#76b461bed081c64908fb1fdfa076ab2c936ca622"
  }
}
